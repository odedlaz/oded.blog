<% if (theme.plugins.katex) { %>
   <%- js('https://cdnjs.cloudflare.com/ajax/libs/KaTeX/0.7.1/katex.min.js') %>
<% } %>

 <% if (theme.plugins.mathjax) { %>
     <script type="text/x-mathjax-config">
         MathJax.Hub.Config({ tex2jax: { inlineMath: [['$','$'], ['\\(','\\)']] } });
     </script>
     <%- js('https://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-MML-AM_CHTML') %>
 <% } %>

<% if (theme.plugins.google_analytics) { %>
    <%- partial('plugin/google-analytics') %>
<% } %>
